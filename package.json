        "apollo-client": "2.6.3",
        "apollo-link": "1.2.13",
        "apollo-link-context": "1.0.19",
        "apollo-link-http": "1.5.15",
        "apollo-link-ws": "1.0.19",
        "apollo-utilities": "1.3.2",
        "graphql": "14.5.8",
        "graphql-tag": "2.10.1",
        "graphql-tag.macro": "2.0.1",
        "graphql.macro": "1.4.2",
        "lodash": "4.17.15",
        "react": "^16.10.2",
        "react-dom": "^16.10.2",
        "react-redux": "^7.1.1",
        "react-router-dom": "^5.1.2",
        "react-scripts": "3.2.0",
        "redux": "^4.0.4",
        "redux-mock-store": "^1.5.3"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "jest": {
        "collectCoverageFrom": [
            "src/**/*.js",
            "!src/index.js",
            "!src/stories/**/*.js",
            "!src/utils.js",
            "!src/timers/**/*.js",
            "!src/basic-mock/**/*.js"
        ],
        "coverageThreshold": {
            "global": {
                "branches": 100,
                "functions": 100,
                "lines": 100,
                "statements": 100
            }
        }
    },
    "devDependencies": {
        "@apollo/react-testing": "3.0.0",
        "@testing-library/react": "9.2.0",
        "babel-jest": "^24.9.0",
        "enzyme": "^3.10.0",
        "enzyme-adapter-react-16": "^1.15.1",
        "enzyme-to-json": "^3.4.3",
        "jest-enzyme": "7.1.1",
        "jest": "^24.9.0",
        "apollo": "2.16.3"
    }
